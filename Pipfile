[scripts]
up = 'docker compose -f "compose.yml" up -d --build'
down = 'docker compose -f "compose.yml" down'
logs = 'docker compose logs'
exec = 'docker compose exec'
migrate = 'docker compose exec django bash -c "pipenv run migrate"' 
start = 'gunicorn umamagic.wsgi:application --bind 0.0.0.0:8001'
migrate_reset = 'bash -c "python manage.py migrate --fake && python manage.py migrate"'
test = 'python manage.py test'


[[source]]
url = "https://pypi.org/simple"
verify_ssl = true
name = "pypi"

[packages]

[dev-packages]

[requires]
python_version = "3"
